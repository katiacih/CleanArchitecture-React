    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "jest-pnp-resolver": "^1.2.2",
    "lint-staged": "^10.2.2",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^25.5.0",
    "ts-loader": "^7.0.4",
